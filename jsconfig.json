{
  "compilerOptions": {
    "jsx": "react",
    "target": "es6",
    "noEmit": true,
    "checkJs": false,
    "baseUrl": ".",
    "paths": {
      "@app/*": ["src/app/*"],
      "@components/*": ["src/components/*"],
      "@common/*": ["src/common/*"],
      "@assets/*": ["src/assets/*"],
      "@data/*": ["src/data/*"]
    }
  },
  "exclude": ["node_modules"]
}
